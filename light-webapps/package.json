{
  "name": "arco-design-pro",
  "version": "1.0.0",
  "description": "Arco Design Pro",
  "license": "ISC",
  "prettier": {
    "singleQuote": true
  },
  "config": {
    "network-timeout": 600000
  },
  "scripts": {
    "start": "cross-env REACT_APP_ENV=production react-app-rewired start",
    "dev": "cross-env REACT_APP_ENV=development react-app-rewired start",
    "build": "cross-env REACT_APP_ENV=production NODE_OPTIONS=--max-old-space-size=4096 react-app-rewired build",
    "eject": "react-scripts eject",
    "format": "eslint --fix \"src/**/*.tsx\"",
    "eslint": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix --cache",
    "stylelint": "stylelint 'src/**/*.less' 'src/**/*.css' --fix --cache",
    "pre-commit": "pretty-quick --staged && npm run eslint && npm run stylelint",
    "build:prod": "cross-env REACT_APP_ENV=production NODE_OPTIONS=--max-old-space-size=4096 react-app-rewired build",
    "build:dev": "cross-env REACT_APP_ENV=development NODE_OPTIONS=--max-old-space-size=4096 react-app-rewired build",
    "build:simu": "cross-env REACT_APP_ENV=simulation NODE_OPTIONS=--max-old-space-size=4096 react-app-rewired build"
  },
  "dependencies": {
    "@arco-design/color": "^0.4.0",
    "@arco-design/web-react": "^2.32.2",
    "@arco-themes/react-arco-pro": "^0.0.7",
    "@arco-themes/react-ldp-theme": "^0.0.18",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
    "@loadable/component": "^5.13.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@turf/turf": "^6.5.0",
    "ace-builds": "^1.31.2",
    "arco-design-pro": "^2.8.0",
    "axios": "^0.24.0",
    "bizcharts": "^4.1.11",
    "brace": "^0.11.1",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^7.0.3",
    "decimal.js": "^10.4.3",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "jsoneditor-react": "^3.1.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mockjs": "^1.1.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "nprogress": "^0.2.0",
    "query-string": "^6.13.8",
    "react": "^16.8.0",
    "react-ace": "^10.1.0",
    "react-color": "^2.18.1",
    "react-dom": "^16.8.0",
    "react-draggable": "^4.4.6",
    "react-icons": "^4.12.0",
    "react-redux": "^8.1.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@arco-plugins/webpack-react": "^1.1.1",
    "@babel/core": "^7.24.0",
    "@svgr/webpack": "^5.5.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "customize-cra": "^1.0.0",
    "customize-cra-less-loader": "^2.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "postcss-less": "^5.0.0",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.2",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^5.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
